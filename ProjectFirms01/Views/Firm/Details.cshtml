@model BusinessLayer.MdlClasses.FirmsBaseModel
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <title>Firm Report</title>
</head>
<body>
    <div class="row" style="text-align:center;">
        <h1> Directorate of FIRMS</h1>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <h1>Firms Info</h1>
                <table style="width:100%;" class="table table-striped table-responsive table-fixedheader  table-bordered dataTable">
                    <thead>
                        <tr>

                            <th>
                                Org Name

                            </th>
                            <th>
                                File #
                            </th>

                        </tr>

                    </thead>
                    <tbody>
                       @foreach (var item in Model.firmsinfomdl) { 
                            <tr>

                                <td>
                                    @item.OrgName

                                </td>
                                <td>
                                    @item.OrgRegNo


                                </td>

                            </tr>
                       }
                    </tbody>



                </table>
            </div>
        <hr />
       
        <div class="col-sm-12">
            <h1>Address</h1>
            <table class="table table-striped table-responsive table-fixedheader  table-bordered dataTable">
                <thead>
                    <tr>

                        <th>
                            OrgRegNo

                        </th>
                        <th>
                            AdressType_ID
                        </th>
                        <th>
                            Adress Details
                        </th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.addresslist)
                    {
                        <tr>

                            <td>
                                @Html.DisplayFor(modelItem => item.OrgRegNo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.AdressType_ID)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.AdressDetails)
                            </td>


                        </tr>
                    }
                </tbody>

            </table>
        </div>
        <hr />
       
        <div class="col-sm-12">
            <h1>Partner</h1>
            <table class="table table-striped table-responsive table-fixedheader  table-bordered dataTable">
                <thead>
                    <tr>
                        <th>
                            File #
                        </th>
                        <th>
                            PartnerName

                        </th>
                        <th>
                            PartnerShare
                        </th>
                        <th>
                            PartnerCNIC
                        </th>


                        <th>
                            PartnerAddress
                        </th>
                        <th>
                            PartnerContactNo
                        </th>
                        <th>
                            PartnerMobileNo
                        </th>
                        <th>
                            IsActive
                        </th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.partnerlist)
                    {
                        <tr>

                            <td>
                                @item.OrgRegNo


                            </td>
                            <td>
                                @item.PartnerName



                            </td>
                            <td>
                                @item.PartnerShare
                            </td>
                            <td>
                                @item.PartnerCNIC


                            </td>
                            <td>
                                @item.PartnerAddress


                            </td>
                            <td>
                                @item.PartnerContactNo

                            </td>
                            <td>
                                @item.PartnerMobileNo
                              

                            </td>
                            <td>
                                @item.IsActive

                            </td>
                        </tr>
                    }
                </tbody>

            </table>
        </div>


        <hr />
        
        <div class="col-sm-12">
            <h1>Witness</h1>
            <table class="table table-striped table-responsive table-fixedheader  table-bordered dataTable">
                <thead>
                    <tr>

                        <th>
                            WitnessName

                        </th>
                        <th>
                            OrgRegNo
                        </th>
                        
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.witnesslist)
                    {
                        <tr>

                            <td>
                                @item.WitnessName


                            </td>
                            <td>
                                @item.OrgRegNo



                            </td>
                            
                        </tr>
                    }
                </tbody>

            </table>
        </div>
    </div>

</body>
</html>
