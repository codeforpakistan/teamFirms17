@model IEnumerable<BusinessLayer.MdlClasses.MdlADDRESSS>

@{
    ViewBag.Title = "Index";
    
 
    Layout = "~/Views/Shared/_FirmLayout.cshtml";
}


@section Header{
    @Html.Partial("~/Views/Shared/PartialViews/_Header.cshtml")
}

@section Sidebar{
    @Html.Partial("~/Views/Shared/PartialViews/_SideBar.cshtml")
}


@section BodyContent{

<ol class="breadcrumb">
    <li><a href="/Home"><i class="fa fa-dashboard"></i>Home</a></li>
    <li><a href="/Firm">Firm</a></li>
    <li class="active">Address</li>
</ol>
 
<p>
    @Html.ActionLink("Add Address", "Create") /
    @Html.ActionLink("Add Partner", "Create","Partner") /
    @Html.ActionLink("Add Witness", "Create","Witness")
   
    @*@TempData["foo"].ToString();*@
</p>
<table id="example1" class="table table-striped table-responsive table-fixedheader  table-bordered dataTable">
    <thead>
        <tr>

            <th>
                Org Name

            </th>
            <th>
                Address Type
            </th>
            <th>
                Adress Details
            </th>
            <th>
                Tehsil
            </th>
            <th>
                Is Active
            </th>

            <th>
                Enter By
            </th>


            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.OrgName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AdressTypeName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AdressDetails)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TehsilName)
                </td>


                <td>
                    @Html.DisplayFor(modelItem => item.IsActive)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.AdressID })
                    @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
                </td>
            </tr>
        }
    </tbody>

</table>

}




@section FooterArea{
    @Html.Partial("~/Views/Shared/PartialViews/_Footer.cshtml")


}