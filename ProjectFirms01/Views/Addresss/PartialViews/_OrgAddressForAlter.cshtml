@model IEnumerable<BusinessLayer.MdlClasses.MdlADDRESSS>

<table class="table table-responsive table-striped table-bordered">
    <tr>
        <th>
            Address ID
        </th>
        <th>
            Address Detail
        </th>

    </tr>
    @foreach (var item in Model)
    {
        <input type="hidden" id="OldAdressId" value="@item.AdressID" name="OldAdressId" />
        <tr>
            <td>
                @item.AdressID
            </td>
            <td>
                @item.AdressDetails
            </td>

        </tr>
    }
</table>


