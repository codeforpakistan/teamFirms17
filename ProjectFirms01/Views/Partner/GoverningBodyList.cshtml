@model IEnumerable<BusinessLayer.MdlClasses.MdlPartner>
@{
    ViewBag.Title = "GoverningBodyList";
    Layout = "~/Views/Shared/_FirmLayout.cshtml";
}

 @section Header{
    @Html.Partial("~/Views/Shared/PartialViews/_Header.cshtml")
}

@section Sidebar{
    @Html.Partial("~/Views/Shared/PartialViews/_SideBar.cshtml")
}


@section BodyContent{
    <ol class="breadcrumb">
        <li><a href="/Home"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="/Madrassa">Madrassa</a></li>        
        <li class="active">Madrassa Governing Body</li>
    </ol>
    <p>
        @Html.ActionLink("Add  Governing Body", "GoverningBody") /
        @Html.ActionLink("Add Witness", "CreateMadrassaWitness", "Witness")
    </p>
    <table id="example1" class="table table-responsive table-striped table-bordered">

        <thead>
            <tr>

                <th>
                    Name
                </th>
                <th>
                    Cnic #
                </th>


                <th>
                    Address

                </th>
                <th>
                    Contact #
                </th>
                <th>
                    Mobile #
                </th>

                <th>
                    OrgDoc_ID
                </th>




                <th>
                    Status
                </th>



                <th>
                    Action
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    @*<td>
                        @Html.DisplayFor(modelItem => item.PartnerID)
                    </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.PartnerName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PartnerCNIC)
                    </td>


                    <td>
                        @Html.DisplayFor(modelItem => item.PartnerAddress)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PartnerContactNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PartnerMobileNo)
                    </td>
                   
                    <td>
                        @Html.DisplayFor(modelItem => item.OrgDoc_ID)
                    </td>

                    <td>
                        <input type="checkbox" checked="@item.IsActive" />
                    </td>

                  
                    <td>
                        @Html.ActionLink("Edit", "MadrassaGoverningBodyEdit", new { id = item.PartnerID })
                        @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
                    </td>
                </tr>
            }
        </tbody>
    </table>

}

@section FooterArea{
    @Html.Partial("~/Views/Shared/PartialViews/_Footer.cshtml")


}
