@model IEnumerable<BusinessLayer.MdlClasses.MdlWitness>
@{
    ViewBag.Title = "SocietyWitnessList";
    Layout = "~/Views/Shared/_FirmLayout.cshtml";
}

 
@section Header{
    @Html.Partial("~/Views/Shared/PartialViews/_Header.cshtml")
}

@section Sidebar{
    @Html.Partial("~/Views/Shared/PartialViews/_SideBar.cshtml")
}


@section BodyContent{
    <ol class="breadcrumb">
        <li><a href="/Home"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="/SocietyNGO">Society/NGO</a></li>  
        <li class="active">Society Witness</li>
    </ol>

    <p>
        @Html.ActionLink("Add Society/NGO Witness", "CreateSocietyWitness")
    </p>
    <table class="table table-responsive table-striped table-bordered datatable">
        <tr>
            @*<th>
                    @Html.DisplayNameFor(model => model.WitnessID)
                </th>*@
            <th>
                @Html.DisplayNameFor(model => model.WitnessName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WitnessCNIC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WitnessONIC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WitnessAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WitnessContactNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WitnessMobileNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrgDoc_ID)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.WitnessPassportNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.WitnessCitizenNo)
                </th>*@
            <th>
                @Html.DisplayNameFor(model => model.EnterByUser_ID)
            </th>

            @*<th>
                    @Html.DisplayNameFor(model => model.EntryDate)
                </th>*@

            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.IsActiveDate)
                </th>*@
            <th>Action</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                @*<td>
                        @Html.DisplayFor(modelItem => item.WitnessID)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.WitnessName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WitnessCNIC)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WitnessONIC)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WitnessAddress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WitnessContactNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WitnessMobileNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OrgDoc_ID)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.WitnessPassportNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.WitnessCitizenNo)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.EnterByUser_ID)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.EntryDate)
                    </td>*@

                <td>
                    @Html.DisplayFor(modelItem => item.IsActive)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.IsActiveDate)
                    </td>*@
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.WitnessID })

                    @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ })|
                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
                </td>
            </tr>
        }

    </table>
}

@section FooterArea{
    @Html.Partial("~/Views/Shared/PartialViews/_Footer.cshtml")


}