@model IEnumerable<BusinessLayer.MdlClasses.MdlOrgType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_FirmLayout.cshtml";
}


@section Header{
    @Html.Partial("~/Views/Shared/PartialViews/_Header.cshtml")
}

@section Sidebar{
    @Html.Partial("~/Views/Shared/PartialViews/_SideBar.cshtml")
}


@section BodyContent{


    <style>
        .form-control {
            width: 400px;
        }
    </style>

<h2>Organization Type</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table id="example1" class="table table-striped table-responsive table-fixedheader  table-bordered">
    <thead>
        <tr>
            <th>
              OrgTypeID
            </th>
            <th>
                Org Type Name
            </th>
            <th>
                IsActive
            </th>
            <th>
                Enter By
            </th>
            <th>
                EntryTime
            </th>
           
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.OrgTypeID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OrgTypeName)
                </td>
                <td>
                    <input type="checkbox" checked="@item.IsActive" />
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EnteredByUser_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EntryTime)
                </td>
               
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.OrgTypeID })
                   
                </td>
            </tr>
        }
    </tbody>
</table>
}

@section FooterArea{
    @Html.Partial("~/Views/Shared/PartialViews/_Footer.cshtml")


}